{
  "extends": ["standard", "standard-react", "standard-jsx"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    // too many false positives
    "react/no-unused-prop-types": 0
  }
}